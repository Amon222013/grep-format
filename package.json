{
  "name": "grep-format",
  "displayName": "grep-format",
  "description": "grep result formatter",
  "version": "0.0.1",
  "publisher": "4mon",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "grep-Format",
      "title": "grep format"
    }],
    "menus": {
      "editor/context": [{
        "when": "editorHasSelection",
        "command": "grepFormat",
        "group": "6_copypath"
      }]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
